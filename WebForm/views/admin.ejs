<body>
    <h2>Start Time</h2>
    <input name="start" type='datetime-local'></input>

    <h2>End Time</h2>
    <input name="datefield" type='datetime-local'></input>


    <div class="container container-small">
        <table class = "table table-sm">
            <thead>
                <tr>
                    <th scope = "col"><h3>Hour</h3></th>
                    <th scope = "col"><h3>Quantity Parked </h3></th>
                </tr>
            </thead>
            <tbody>
                <% for (var i = 0; i < hours.length; i++){ %>
                     <tr> 
                        <td><%- hours[i] %></td>
                        <td><%- quants[i] %></td>
                     </tr>
                <% } %>
            </tbody>
        </table>

